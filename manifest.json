{
	"manifest_version": 2,
	"name": "Site Deck",
	"description": "Cycle through different urls at specified intervals",
	"version": "1.1.7",
	"background": {
		"page": "background.html"
	},

	"options_page": "options.html",

	"permissions":[
		"tabs"
	],

	"icons": {
		"16": "icons/icon16.png",
		"19": "icons/icon19.png",
		"48": "icons/icon48.png",
	  "128": "icons/icon128.png"
	},

	"browser_action": {
		"default_title": "WebFrame",
		"default_icon": {
       	"16": "icons/icon16.png",
			"19": "icons/icon19.png",
			"48": "icons/icon48.png",
		  "128": "icons/icon128.png"
		}
	}
}
